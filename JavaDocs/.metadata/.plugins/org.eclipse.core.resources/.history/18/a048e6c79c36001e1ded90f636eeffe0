package com.pracitce.program;

public class StringSwap {

	public static void main(String[] args) {
		
		String first = "Vijay";
		String last = "Chaitanya";
		
		
		
		 first = first+last;
		 
		 last =first.substring(0,last.length()-1);
		 
		 System.out.println(last);
		
		

	}

}
