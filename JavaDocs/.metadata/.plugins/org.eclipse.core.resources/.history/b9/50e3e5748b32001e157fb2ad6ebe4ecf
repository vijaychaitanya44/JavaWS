package pack1.program;

public class TwoArrayCopy {

	public static void main(String[] args) {
		
			      
			      
			       int arr1[] = {4,5,6,1,2};
			       int arr2[] = {0,22,3,8};
			       
			       int l1 = arr1.length;
			       int l2 = arr2.length;
			       int res[]=new int[l1+l2];
			       
	System.arraycopy(arr1, 0, res, 0, l1);
	
	System.arraycopy(arr2, 0, res, l1, l2);
	
	for(int a:res) {
		System.out.println(a);
	}
	
	}
}
