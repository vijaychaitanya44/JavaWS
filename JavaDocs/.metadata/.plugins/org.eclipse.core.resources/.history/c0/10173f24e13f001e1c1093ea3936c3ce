package programs;

public class RemoveSpecial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String str = "abcd %lmd# yu";

		for (int i = 0; i < str.length(); i++) {
			char c = str.charAt(i);
			if (c >= 'a' && c <= 'z') {

				System.out.println(c);
			}
		}

	}

}
